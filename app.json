{
  "expo": {
    "name": "my-react-native-app",
    "description": "This is testing app for me for learning react native with their tools and other stuffs releated to react native",
    "slug": "my-react-native-app",
    "owner": "Pushpaje",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/react-native-icon.png",
    "scheme": "myreactnativeapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/react-native-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    //  [
    //   "expo-navigation-bar",
    //   {
    //     "backgroundColor": "#0f172a",
    //       "barStyle": "light",
    //       "borderColor": "#1f2937",
    //       "visibility": "visible",
    //       "behavior": "inset-swipe",
    //       "position": "relative"
    //   }
    //  ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
